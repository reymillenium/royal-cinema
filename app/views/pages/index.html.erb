<!-- services -->
<%= render partial: "services" %>
<!-- //services -->

<!-- interests -->
<%= render partial: "interests" %>
<!-- //interests -->

<!-- experience -->
<%= render partial: "experience" %>
<!-- //experience -->

<!-- education -->
<%= render partial: "education" %>
<!-- //education -->

<!-- projects -->
<%= render partial: "projects" %>
<!-- //projects -->

<!-- references -->
<%= render partial: "references", locals: { references: references } %>
<!-- //references -->

<!-- samples -->
<%= render partial: "samples" %>
<!-- //samples -->

<!-- introduce -->
